<Window x:Class="FinalProjectWPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FinalProjectWPF"
        mc:Ignorable="d"
        Title="Project Center" Height="600" Width="800"
        WindowStartupLocation="CenterScreen"
        Icon="Assets\ALogo.png">
    <Grid>
        <Grid.Background>
            <LinearGradientBrush StartPoint="0,0" EndPoint="1,1">
                <GradientStop Color="#305496" Offset="0"/>
                <GradientStop Color="#D9E1F2" Offset="1"/>
            </LinearGradientBrush>
        </Grid.Background>
        <Frame x:Name="MainFrame" NavigationUIVisibility="Hidden"/>
    </Grid>
</Window>
